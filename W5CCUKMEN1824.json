{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  
        "data": {
        "url": "https://api.allorigins.win/raw?url=https://api.ons.gov.uk/timeseries/jlgg/dataset/unem/data",
        "format": {
          "type":"json",
          "property": "months"}},
  
  
       "mark":{
        "type": "line",
        "color":"darkred",
        "strokeWidth":1,
        "opacity":1},
  
      "title": {
      "text": "UK - Men aged 18-24 - SA",
      "subtitle": "Claimant Count % of population. > 12 months",
      "subtitleFontStyle":"italic",
      "subtitleFontSize":10,
      "anchor": "start",
      "color": "black"},
  
      "width":200,
      "height":200,
  
      "encoding": {
        "x": {
          "field": "date",
          "type": "temporal",
          "title":null,
          "axis": {"grid": false, "ticks": false}
          },
  
        "y": {
          "field": "value",
          "type": "quantitative",
          "title":null,
          "axis": {"grid": false, "ticks": false}
          },
          
  
          "tooltip": [
  
        {"field": "date", "type": "temporal", "title": "Date"},
  
        {"field": "value", "type": "quantitative", "title": "Claimant Count %"}
  
     
  
      ]
      }
  }
  